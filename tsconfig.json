{
    "compilerOptions": {
        "outDir": "./dist",
         "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "typeRoots": [
        "node_modules/@types"
        ],
        "types": [
        "node",
            "core-js"
        ]
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
    "node_modules",
    "dist",
    "build"
]
}
